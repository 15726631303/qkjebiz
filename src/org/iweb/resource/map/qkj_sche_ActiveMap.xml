<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "sql-map-2.dtd">
<sqlMap>
	<typeAlias alias="schedule" type="com.qkj.sche.domain.Schedule" />
	<select id="sche_getSche_ss" resultClass="schedule" parameterClass="java.util.Map">
		<![CDATA[ 
		SELECT g.* FROM qkj_t_schedule g
		]]>
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="ssid"><![CDATA[ g.ssid=#ssid# ]]></isNotNull>
			<isNotNull prepend="AND" property="title"><![CDATA[ g.title=#title# ]]></isNotNull>
		</dynamic>
	</select>
	<insert id="sche_addSche_ss" parameterClass="schedule">
		<![CDATA[ 
			Insert Into qkj_t_schedule(title,content,sdate)
			Values (#title#,#content#,#sdate#)
		]]>
	</insert>
	<update id="sche_saveSche_ss" parameterClass="schedule">
		<![CDATA[
			Update qkj_t_schedule  
			Set titile=#title#,
				content=#content#,
				sdate=#sdate#,
			Where ssid=#ssid#
		]]>
	</update>
	<delete id="sche_delSche_ss" parameterClass="schedule">
		<![CDATA[
			Delete From qkj_t_schedule
		]]>
		<![CDATA[ Where ssid In ]]>
		<iterate property="ssid" open="(" close=")" conjunction=" , "> #ssid[]#</iterate>
	</delete>
</sqlMap>